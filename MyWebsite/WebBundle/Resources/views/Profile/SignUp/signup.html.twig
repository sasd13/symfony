{% extends "MyWebsiteWebBundle::layout.html.twig" %}

{% block title %}{{ parent() }} - Inscription{% endblock %}

{% block css %} 
	{% stylesheets
			'bundles/mywebsiteweb/css/form-style.css'
			filter='cssrewrite' %}
			<link rel="stylesheet" href="{{ asset_url }}" type="text/css" />
	{% endstylesheets %}
{% endblock %}

{% block class_header %}header2{% endblock %}

{% block section %}
	<!---Start-content---->
	<div class="content">
		<div class="wrap">
			<!---start-Features---->
			<div class="contact-form">
				<div class="feature-grids">
					<div class="feature-grid-head">
						<h3><span>Sign up to</span> MyProfil</h3>
						<div class="feature-grid-head-top">
							<label> </label><span> </span> <label> </label>
							<div class="clear"> </div>
						</div>
					</div>
					<div class="new-form">
						{% include "MyWebsiteWebBundle:Profile:SignUp/" ~ subLayout ~ ".html.twig" %}
					</div>
				</div>
			</div>
		</div>
	</div>
	<!---End-content---->
{% endblock %}